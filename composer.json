{
    "name": "MylesDC/chat",
    "description": "Chat Package for Laravel 5.4 and up with Archive conversation feature.",
    "type": "library",
    "keywords": [
        "laravel",
        "chat",
        "messaging",
        "conversation"
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "~5.4.0|~5.5.0|~5.6.0",
        "illuminate/config": "~5.4.0|~5.5.0|~5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|6.2|^7.0",
        "orchestra/testbench": "~3.3.0|~3.4.2|^3.5.0",
        "orchestra/database": "~3.3.0|~3.4.2|^3.5.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tinashe MylesDC",
            "email": "tashtin263@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "MylesDC\\Chat\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MylesDC\\Chat\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MylesDC\\Chat\\ChatServiceProvider"
            ]
        }
    }
}